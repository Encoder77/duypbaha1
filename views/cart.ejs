<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head') %>
<body id="default_theme" class="it_service" lang="<%- lang %>">
<%- include('partials/header') %>

<div class="shopping-cart">

  <div class="column-labels">
    <label class="product-image" >Image</label>
    <label class="product-details" >Product</label>
    <label class="product-price" data-key="cena" >Price</label>
    <label class="product-quantity" data-key="mukdar" >Quantity</label>
    <label class="product-removal" data-key="delete" >Remove</label>
    <label class="product-line-price" data-key="jemi" >Total</label>
  </div>
<div class="cart">
</div>

<div class="totals">
    <div class="totals-item">
      <label data-key="subtotal" >Subtotal</label>
      <div class="totals-value" id="cart-subtotal">0</div>
    </div>
    <div class="totals-item">
      <label data-key="dostawka" >Shipping</label>
      <div class="totals-value" id="cart-shipping">15.00</div>
    </div>
    <div  class="totals-item totals-item-total">
      <label data-key="umumyjem" >Grand Total</label>
      <div class="totals-value" id="cart-total">90.57</div>
    </div>
</div>
      
      <button class="checkout" data-key="sargytet" >Checkout</button>

</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script src="/js/cart_page.js" ></script>
<script src="/js/lang.js"></script>
<script src="/js/lang-switcher.js"></script>
<%- include('partials/footer') %>


</body>
</html>
